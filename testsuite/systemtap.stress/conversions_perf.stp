		print (user_int_warn ($1))
	}
}
probe perf.sw.cpu_clock {
	if (hits[23] == 0) {
		hits[23] = 1
		print (user_long ($1))
	}
}
probe perf.sw.cpu_clock {
	if (hits[24] == 0) {
