   if(f & 0x4000) bs="MSG_NOSIGNAL|".bs
   if(f & 0x0800) bs="MSG_CONFIRM|".bs
   if(f & 0x8000) bs="MSG_MORE|".bs
   return substr(bs,0,strlen(bs)-1)
}

