probe nfs.fop.llseek {
	printf("%s(%s)\n", name, argstr)
	printf("%d %d %d %s %d\n", dev, ino, s_id, devname, maxbyte)
}
probe nfs.fop.llseek.return {
	printf("%s(%s)\n", name, retstr)
}

probe nfs.fop.read, nfs.fop.write {
	printf("%s(%s)\n", name, argstr)
	printf("%d %s\n", s_id, devname)
}
