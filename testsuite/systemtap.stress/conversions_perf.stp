		print (user_string_quoted ($1))
	}
}
probe perf.sw.cpu_clock {
	if (hits[14] == 0) {
		hits[14] = 1
		print (user_string_n ($1, 5))
	}
}
probe perf.sw.cpu_clock {
	if (hits[15] == 0) {
		hits[15] = 1
		print (user_string_n ($1, 5))
	}
}
probe perf.sw.cpu_clock {
	if (hits[16] == 0) {
		hits[16] = 1
		print (user_string_n2 ($1, 5, "<only suspected, not known>"))
	}
}
probe perf.sw.cpu_clock {
	if (hits[17] == 0) {
		hits[17] = 1
		print (user_string_n_warn ($1, 5))
	}
}
probe perf.sw.cpu_clock {
	if (hits[18] == 0) {
		hits[18] = 1
		print (user_string_n_quoted ($1, 5))
 	}
}
probe perf.sw.cpu_clock {
