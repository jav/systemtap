# test of histogram with interval too small

set test "linear_bad"

if {![installtest_p]} { untested $test; return }

set ok 0

spawn stap -DMAXACTION=10000 $srcdir/$subdir/$test.stp
expect {
    -timeout 150
    "WARNING: histogram: Interval is too small." {
	incr ok
	exp_continue
    }
    eof { }
}
catch close
wait

if {$ok == 1} { 
    pass $test 
} else { 
    fail $test 
}
