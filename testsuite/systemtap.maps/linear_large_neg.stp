global agg

probe begin
{
	# Add items to the aggregate

	agg <<< 0

	agg <<< -100000000
	agg <<< -100000000

	agg <<< -500000000
	agg <<< -500000000
	agg <<< -500000000
	agg <<< -500000000
	agg <<< -500000000

	agg <<< -1000000000
	agg <<< -1000000000
