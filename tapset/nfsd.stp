        version = 2
	fh = __get_fh($argp,3)

	count = $argp->count 
	offset = $argp->offset 
	vec = $argp->vec
	vlen = $argp->vlen

	name = "nfsd.proc2.read"
	argstr = sprintf("%d,%d",count,offset)

        size = count
        units = "bytes"
}

 
