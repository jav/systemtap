		text_strn(user_string(payload_uaddr), syscall_string_trunc, 1),
		plen, ringid)
}
probe nd_syscall.add_key.return = kprobe.function("sys_add_key").return ?
{
	name = "add_key"
	retstr = returnstr(1)
}

