	if (flags & SOCK_CLOEXEC) {
		strlcat (STAP_RETVALUE, "|SOCK_CLOEXEC", MAXSTRINGLEN);
	}
	if (flags & SOCK_NONBLOCK) {
		strlcat (STAP_RETVALUE, "|SOCK_NONBLOCK", MAXSTRINGLEN);
	}
#endif
%}

