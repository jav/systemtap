#! stap -p4

#
# nfsd.dispatch tests
#

probe nfsd.dispatch
{
	printf("%s(%s)\n", name, argstr)
	printf("%s %d %d %d\n", client_ip, xid, prog, proc)
}
probe nfsd.dispatch.return
{
	printf("%s(%s)\n", name, retstr)
}

